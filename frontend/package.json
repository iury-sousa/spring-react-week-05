{
  "name": "dsvendas",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "type-check": "tsc",
    "build": "tsc && vite build && shx cp _redirects ./dist/_redirects",
    "serve": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "apexcharts": "^3.29.0",
    "bootstrap": "^5.1.3",
    "react": "^17.0.0",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "eslint": "^8.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "shx": "^0.3.3",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --cache --fix",
      "git add"
    ]
  }
}
